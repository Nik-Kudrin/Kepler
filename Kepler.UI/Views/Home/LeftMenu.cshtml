<script type="text/javascript">
    $(document).ready(function () {
        $('.left-nav-link').click(function () {

            $('.left-nav-link').removeClass("current");
            $(this).addClass("current");

            var linkUrl = $(this).attr('linkUrl');

            $.ajax({
                url: linkUrl,
                type: 'GET',
                success: function (data) {
                    $('#page-inner').html(data);
                },
                complete: function (data) {
                    document.location.hash = $('.left-nav-link.current').attr("linkBranch") + "_branch"; //set HASH to url - e.g. #Develop_branch etc.
                    $('.panel-heading h3').text($('.left-nav-link.current').attr("linkBranch"));
                }
            });
        });

        $('.main-menu-conf a').click(function () {
            $('.main-menu-conf a').removeClass("current");
            $(this).addClass("current");
        });

    });

</script>